@Html.Hidden("RedirectToLogin", Url.Action("Index", "Home", new { area = "" }))
<script>
    function LogoutFun() {

        $.ajax({
            type: "POST",
            url: "/Admin/Admin/LogOff",
            data: {
                returnUrl: 'v'
            },
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (data) {
                var url = $("#RedirectToLogin").val();
                location.href = url;
            },

        });
    }
</script>
